{
  "name": "egg-kmore",
  "author": "waiting",
  "version": "2.0.1",
  "description": "egg plugin of kmore",
  "eggPlugin": {
    "name": "kmore"
  },
  "keywords": [
    "midway",
    "egg",
    "eggPlugin",
    "egg-plugin",
    "kmore",
    "knex",
    "typeorm",
    "database",
    "postgresql",
    "oracle",
    "typescript",
    "ts"
  ],
  "bin": {},
  "browser": "",
  "es2015": "",
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bugs": {
    "url": "https://github.com/waitingsong/kmore/issues"
  },
  "homepage": "https://waitingsong.github.io/kmore/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/waitingsong/kmore.git"
  },
  "license": "MIT",
  "dependencies": {
    "@waiting/shared-types": "4",
    "kmore": "^2.0.1",
    "kmore-cli": "^2.0.1"
  },
  "devDependencies": {
    "cross-env": "7",
    "egg": "2",
    "knex": "^0.21.0",
    "pg": "^8.2.1"
  },
  "peerDependencies": {
    "knex": ">=0.21.0",
    "typescript": "3"
  },
  "engines": {
    "node": ">=10.16.0"
  },
  "files": [
    "agent.js",
    "app.js",
    "app",
    "config",
    "dist",
    "*.d.ts"
  ],
  "scripts": {
    "build": "npm run tsc && npm run rp",
    "clean": "rm -rf dist/* .vscode/.tsbuildinfo .vscode/.tsbuildinfo.* ",
    "cov": "cross-env TS_NODE_PROJECT=test/tsconfig.json nyc mocha",
    "lint": "eslint --fix --cache {src,test}/**/*.ts",
    "lint:nofix": "eslint --cache {src,test}/**/*.ts",
    "rp": "rollup -c rollup.config.js",
    "rp:w": "npm run tsc:w | rollup -wc rollup.config.js",
    "test": "cross-env TS_NODE_PROJECT=test/tsconfig.json mocha",
    "test:browser": "cd .config && npm run browser && cd ..",
    "tsc": "tsc -p tsconfig.json",
    "tsc:w": "tsc -p tsconfig.json -w"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      ".githooks",
      "dist",
      "node_modules*",
      "src/bin",
      "src/domain.ts",
      "src/interface.ts",
      "test*",
      "**/node_modules*",
      "**/*.d.ts",
      "**/*.js"
    ],
    "extension": [
      ".ts"
    ],
    "reporter": [
      "text-summary",
      "json",
      "html"
    ],
    "all": true
  }
}
