{
  "name": "kmore-cli",
  "author": "waiting",
  "version": "27.0.0",
  "description": "Command line to generate dicts from ts source files for kmore",
  "keywords": [
    "kmore",
    "knex",
    "typescript",
    "ts"
  ],
  "bin": {
    "kmore": "./dist/bin/kmore-cli.js"
  },
  "browser": "",
  "es2015": "",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "bugs": {
    "url": "https://github.com/waitingsong/kmore/issues"
  },
  "homepage": "https://waitingsong.github.io/kmore/",
  "repository": {
    "type": "git",
    "url": "https://github.com/waitingsong/kmore"
  },
  "license": "MIT",
  "dependencies": {
    "@waiting/shared-core": "^19.1.0",
    "@waiting/shared-types": "^19.0.1",
    "@waiting/shared-types-dev": "^10.0.2",
    "kmore-types": "^27.0.0",
    "minimist": "^1.2.6",
    "rxjs": "^7.5.0",
    "rxwalker": "^6.0.0",
    "tsconfig-resolver": "^3.0.1"
  },
  "devDependencies": {
    "cross-env": "7",
    "rxrunscript": "^11.0.1"
  },
  "engines": {
    "node": ">=16.13.0"
  },
  "files": [
    "tsconfig.json",
    "src/**/*.ts",
    "bin/*.js",
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run tsc && npm run rp",
    "clean": "npm run clean:lock & npm run clean:cache & rm -rf dist/* -rf",
    "clean:cache": "rm -rf .eslintcache .vscode/.tsbuildinfo .vscode/.tsbuildinfo.*",
    "clean:lock": "rm package-lock.json -f",
    "cov": "cross-env TS_NODE_PROJECT=test/tsconfig.json c8 mocha --parallel=false",
    "lint": "eslint --fix --cache {src,test}/**/*.ts",
    "lint:nofix": "eslint --cache {src,test}/**/*.ts",
    "prepublishOnly": "sh ../../.scripts/change-pkg-types-dist.sh",
    "postpublish": "sh ../../.scripts/change-pkg-types-src.sh",
    "purge": "npm run clean && rm node_modules -rf",
    "rp": "rollup -c rollup.config.js --context this",
    "rp:w": "npm run tsc:w | rollup -wc rollup.config.js",
    "test": "cross-env TS_NODE_PROJECT=test/tsconfig.json mocha",
    "test:browser": "cd .config && npm run browser && cd ..",
    "test:local": "cross-env TS_NODE_PROJECT=test/tsconfig.json ../../node_modules/.bin/mocha --parallel=false",
    "tsc": "tsc -p tsconfig.json",
    "tsc:w": "tsc -p tsconfig.json -w"
  }
}
