{
  "name": "@mw-components/kmore",
  "author": "waiting",
  "version": "22.3.0",
  "description": "midway component for knex",
  "keywords": [
    "midway",
    "IoC",
    "plugin",
    "knex",
    "typeorm",
    "sql",
    "database",
    "postgresql",
    "oracle",
    "ts"
  ],
  "bin": {},
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bugs": {
    "url": "https://github.com/waitingsong/kmore/issues"
  },
  "homepage": "https://waitingsong.github.io/kmore/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/waitingsong/kmore.git"
  },
  "license": "MIT",
  "dependencies": {
    "@mw-components/jaeger": "^7.0.0",
    "@waiting/shared-core": "^14.4.1",
    "@waiting/shared-types": "^14.4.1",
    "kmore": "^22.2.0",
    "knex": "^0.95.0",
    "rxjs": "7",
    "tsconfig-paths": "3"
  },
  "devDependencies": {
    "@midwayjs/core": "^2.11.0",
    "@midwayjs/decorator": "^2.11.0",
    "@midwayjs/koa": "^2.11.0",
    "@midwayjs/mock": "^2.11.0",
    "@midwayjs/web": "^2.11.0",
    "cross-env": "7",
    "egg": "^2.29.0",
    "kmore-types": "^22.0.0"
  },
  "engines": {
    "node": ">=14.16.0"
  },
  "files": [
    "bin/*.js",
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run tsc",
    "clean": "npm run clean:lock & npm run clean:cache & rm -rf dist/* -rf",
    "clean:cache": "rm -rf .eslintcache .vscode/.tsbuildinfo .vscode/.tsbuildinfo.*",
    "clean:lock": "rm package-lock.json -f",
    "cov": "",
    "lint": "eslint --fix --cache {src,test}/**/*.ts",
    "lint:nofix": "eslint --cache {src,test}/**/*.ts",
    "purge": "npm run clean && rm node_modules -rf",
    "test": "",
    "tsc": "tsc -p tsconfig.json"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      ".githooks",
      "dist",
      "node_modules*",
      "src/bin",
      "src/domain.ts",
      "src/interface.ts",
      "test*",
      "**/node_modules*",
      "**/*.d.ts",
      "**/*.js"
    ],
    "extension": [
      ".ts"
    ],
    "reporter": [
      "html",
      "json",
      "text",
      "text-summary"
    ],
    "all": true
  }
}
